@{
    Layout = null;
}

<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8" />
    <title>施工日志</title>
    <meta name="viewport" content="width=device-width,
                                     initial-scale=1.0,
                                     maximum-scale=1.0,
                                     user-scalable=no">
    <link rel="stylesheet" type="text/css" href="~/Content/css/bootstrap/bootstrap.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/css/contractContent.css" />
</head>

<body style="padding: 0; margin: 0;">
    <div class="container" style="margin: 0; width: auto;">
        <div class="row">
            <div>
                <div style="padding: 1em;">
                    <h3 id="lbl_logName">日志1</h3>
                    <hr style="border: 1px solid gray;" />
                    <div class="up-to-date">
                        <label class="brlabel-lg" id="lbl_contractName">合同名称：</label>
                        <label class="brlabel-lg" id="lbl_services">服务款项：</label>
                        <label class="brlabel-lg" id="lbl_projectStart"></label>
                        <label class="brlabel-lg" id="lbl_pompletedDate"></label>
                        <label class="brlabel-lg" id="lbl_pompletedAcceptanceDate"></label>
                    </div>
                    <hr style="border: 1px solid gray;" />
                    <div class="text-center">
                        <button class="btn btn-primary btn-full-width" type="button" onclick="location.href = 'Project'">返回</button>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <script src="~/Scripts/js/projectLog.js" type="text/javascript" charset="utf-8"></script>
    <script src="~/Scripts/js/bootstrap/jquery-3.2.1.slim.min.js" type="text/javascript" charset="utf-8"></script>
    <script src="~/Scripts/js/bootstrap/bootstrap.js" type="text/javascript" charset="utf-8"></script>
    <script type="text/javascript">
        var ContractName = "@ViewBag.ContractName";
        @model ContractStatementManagementSystem.ProjectLog
        var LogName= "@Model.LogName";
        var ProjectStart= "@Model.ProjectStart";
        var Service= "@Model.Service";
        var DompletedDate="@Model.DompletedDate";
        var DompletedAcceptanceDate="@Model.DompletedAcceptanceDate";

    </script>
</body>

</html>