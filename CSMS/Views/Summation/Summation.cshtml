@{
    Layout = null;
}
<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8" />
    <title>汇总</title>
    <meta name="viewport" content="width=device-width,
                                     initial-scale=1.0,
                                     maximum-scale=1.0,
                                     user-scalable=no">
    <link rel="stylesheet" type="text/css" href="~/Content/css/bootstrap/bootstrap.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/css/mmenu/jquery.mmenu.all.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/css/timeline/timeline.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/css/contractContent.css" />
</head>

<body style="padding: 0; margin: 0;">
    <div class="container" style="margin: 0; width: auto;">
        <div class="row">
            <div>
                <div class="menu">
                    <a href="#menu"><span class="glyphicon glyphicon-align-justify menu-icon"></span></a>
                </div>
                <div class="content">
                    <h3>合同信息汇总</h3>
                    <button onclick="window.print()">打印</button>
                    <hr style="border: 1px solid gray;" />
                    <div class="panel-group" id="accordion">

                        <label class="brlabel-lg" id="lbl_contractName">合同名称：</label>
                        <label class="brlabel-lg" id="lbl_costumerName">合同方客户名称：</label>
                        <label class="brlabel-lg" id="lbl_contractType">合同类别：</label>
                        <label class="brlabel-lg" id="lbl_contractAmount">合同金额（人民币元）：</label>
                        <label class="brlabel-lg" id="lbl_count">数量（套/个）：</label>
                        <label class="brlabel-lg" id="lbl_contractNumber">合同编号：</label>
                        <label class="brlabel-lg" id="lbl_signatureDate">合同签署日期：</label>
                        <div class="form-group">
                            <label class="control-label brlabel-lg" for="txt_services">服务款项：</label>
                            <select class="form-control" style="max-width: 30em;" id="txt_services" name="txt_services" onchange="serviceChanged(this.value);"></select>
                        </div>
                        <div class="panel-group" id="accordion">
                            <div class="panel panel-success">
                                <div style="cursor: pointer" class="panel-heading">
                                    <h4 class="panel-title">
                                        <div data-toggle="collapse" data-parent="#accordion"
                                           href="#production">
                                            生产
                                        </div>
                                    </h4>
                                </div>
                                <div id="production" class="panel-collapse collapse in">
                                    <div class="panel-body">
                                        <div class="up-to-date">
                                            <label class="brlabel-lg" id="lbl_totalProduc">已生产量：</label>
                                            <label class="brlabel-lg" id="lbl_noTotalProduc">未生产量：</label>
                                            <label class="brlabel-lg" >生产日志：</label>
                                            <table class="table table-bordered">

                                                <thead>
                                                    <tr>
                                                        <th>生产说明</th>
                                                        <th>生产量</th>
                                                        <th>生产日期</th>
                                                        <th>日志日期</th>
                                                        <th>操作人</th>
                                                    </tr>
                                                </thead>
                                                <tbody id="t"></tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="panel panel-success">
                                <div style="cursor: pointer" class="panel-heading">
                                    <h4 class="panel-title">
                                        <div data-toggle="collapse" data-parent="#accordion"
                                           href="#warehouse">
                                            发货
                                        </div>
                                    </h4>
                                </div>
                                <div id="warehouse" class="panel-collapse collapse in">
                                    <div class="panel-body">
                                        <div class="up-to-date">

                                            <label class="brlabel-lg" id="lbl_reserves">库存量：</label>
                                            <label class="brlabel-lg" id="lbl_shippedCount">已发货数量：</label>
                                            <label class="brlabel-lg" id="lbl_noShippedCount">未发货数量：</label>
                                            <label class="brlabel-lg" >发货日志：</label>
                                            <table class="table table-bordered">

                                                <thead>
                                                    <tr>
                                                        <th>发货说明</th>
                                                        <th>发货量</th>
                                                        <th>发货日期</th>
                                                        <th>日志日期</th>
                                                        <th>操作人</th>
                                                    </tr>
                                                </thead>
                                                <tbody id="t2"></tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="panel panel-success">
                                <div style="cursor: pointer" class="panel-heading">
                                    <h4 class="panel-title">
                                        <div data-toggle="collapse" data-parent="#accordion"
                                           href="#project">
                                            施工
                                        </div>
                                    </h4>
                                </div>
                                <div id="project" class="panel-collapse collapse in">
                                    <div class="panel-body">
                                        <div class="up-to-date">

                                            <label class="brlabel-lg" id="lbl_projectStart">施工日期：</label>
                                            <label class="brlabel-lg" id="lbl_pompletedDate">竣工日期：</label>
                                            <label class="brlabel-lg" id="lbl_pompletedAcceptanceDate">取得竣工验收单日期：</label>
                                            <label class="brlabel-lg">施工日志：</label>
                                            <table class="table table-bordered">

                                                <thead>
                                                    <tr>
                                                        <th>施工说明</th>
                                                        <th>服务款项</th>
                                                        <th>日期修改</th>
                                                        <th>日志日期</th>
                                                        <th>操作人</th>
                                                    </tr>
                                                </thead>
                                                <tbody id="t3"></tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="panel panel-success">
                                <div style="cursor: pointer" class="panel-heading">
                                    <h4 class="panel-title">
                                        <div data-toggle="collapse" data-parent="#accordion"
                                           href="#sales">
                                            收款
                                        </div>
                                    </h4>
                                </div>
                                <div id="sales" class="panel-collapse collapse in">
                                    <div class="panel-body">
                                        <div class="up-to-date">

                                            <label class="brlabel-lg" id="lbl_amountReceived">已收金额：</label>
                                            <label class="brlabel-lg" id="lbl_amountNotReceived">未收金额：</label>
                                            <label class="brlabel-lg" >收款日志：</label>
                                            <table class="table table-bordered">

                                                <thead>
                                                    <tr>
                                                        <th>收款说明</th>
                                                        <th>收款日期</th>
                                                        <th>收款金额</th>
                                                        <th>服务条款</th>
                                                        <th>日志日期</th>
                                                        <th>操作人</th>
                                                    </tr>
                                                </thead>
                                                <tbody id="t4"></tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="panel panel-success">
                                <div style="cursor: pointer" class="panel-heading">
                                    <h4 class="panel-title">
                                        <div data-toggle="collapse" data-parent="#accordion"
                                           href="#Invoicing">
                                            开票
                                        </div>
                                    </h4>
                                </div>
                                <div id="Invoicing" class="panel-collapse collapse in">
                                    <div class="panel-body">
                                        <div class="up-to-date">
                                            <label class="brlabel-lg" id="lbl_subInvoiceCount">已开票数量：</label>
                                            <label class="brlabel-lg" id="lbl_subInvoiceAmount">已开票金额：</label>
                                            <label class="brlabel-lg" >开票日志：</label>
                                            <table class="table table-bordered">

                                                <thead>
                                                    <tr class="">
                                                        <th>开票说明</th>
                                                        <th>开票数量</th>
                                                        <th>开票金额</th>
                                                        <th>开票日期</th>
                                                        <th>服务条款</th>
                                                        <th>日志日期</th>
                                                        <th>操作人</th>
                                                    </tr>
                                                </thead>
                                                <tbody id="t5"></tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="panel panel-success">
                                <div style="cursor: pointer" class="panel-heading">
                                    <h4 class="panel-title">
                                        <div data-toggle="collapse" data-parent="#accordion"
                                           href="#accountant">
                                            结算
                                        </div>
                                    </h4>
                                </div>
                                <div id="accountant" class="panel-collapse collapse in">
                                    <div class="panel-body">
                                        <div class="up-to-date">
                                            <label class="brlabel-lg" id="lbl_affirmIncomeGist">确认收入依据：</label>
                                            <label class="brlabel-lg" id="lbl_AffirmIncomeAmount">确认收入金额：</label>
                                            <label class="brlabel-lg" id="lbl_subCost">已结转成本数量：</label>
                                            <label class="brlabel-lg" id="lbl_subworker">直接人工：</label>
                                            <label class="brlabel-lg" id="lbl_subMaterial">直接材料：</label>
                                            <label class="brlabel-lg" id="lbl_subtotal">小计：</label>
                                            <label class="brlabel-lg" id="lbl_avgGrossrofitMargin">2017年1-12月毛利率：</label>
                                            <label class="brlabel-lg" >结算日志：</label>
                                            <table class="table table-bordered">

                                                <thead>
                                                    <tr>
                                                        <th>结算说明</th>
                                                        <th>结算的修改</th>
                                                        <th>服务条款</th>
                                                        <th>日志日期</th>
                                                        <th>操作人</th>
                                                    </tr>
                                                </thead>
                                                <tbody id="t6"></tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <hr style="border: 1px solid gray;" />
                        </div>
                    </div>
                </div>
                <nav class="nav-hidden" id="menu">
                    <ul>
                        <li><a href="~/Contract/contractContent">合同内容</a></li>
                        <li><a href="~/Production/Production">生产</a></li>
                        <li><a href="~/Warehouse/Warehouse">发货</a></li>
                        <li><a href="~/Project/Project">施工</a></li>
                        <li><a href="~/Sales/Sales">收款</a></li>
                        <li>
                            <a href="~/Invoicing/Invoicing">开票</a>
                        </li>
                        <li><a href="~/Accountant/Accountant">结算</a></li>
                        <li><a href="#" onclick="return false;">合同汇总</a></li>
                        <div class="text-center">
                            <button class="btn btn-primary btn-back" type="button" onclick="location.href = '/Contract/Index'">返回</button>
                        </div>
                    </ul>
                </nav>
            </div>
        </div>
        <script src="http://g.alicdn.com/dingding/open-develop/1.6.9/dingtalk.js" type="text/javascript" charset="utf-8"></script>
        <script src="https://g.alicdn.com/dingding/dingtalk-pc-api/2.7.0/index.js" type="text/javascript" charset="utf-8"></script>
        <script src="~/Scripts/js/bootstrap/jquery.min.js" type="text/javascript" charset="utf-8"></script>
        <script src="~/Scripts/js/premissions.js" type="text/javascript" charset="utf-8"></script>
        <script src="~/Scripts/js/bootstrap/bootstrap.js" type="text/javascript" charset="utf-8"></script>
        <script src="~/Scripts/js/mmenu/jquery.mmenu.all.js" type="text/javascript" charset="utf-8"></script>
        <script src="~/Scripts/js/timeline/modernizr.js" type="text/javascript" charset="utf-8"></script>
        <script src="~/Scripts/js/Summation.js" type="text/javascript" charset="utf-8"></script>
        <script type="text/javascript">
            var ProductionerJson = "@ViewBag.ProductionerJson";
            var WarehouseJson = "@ViewBag.WarehouseJson";
            var ProjectJson = "@ViewBag.ProjectJson";
            var SalesJson = "@ViewBag.SalesJson";
            var AccountantJson = "@ViewBag.AccountantJson";
            var ContractNameTJson = "@ViewBag.ContractNameTJson";
            var InvoicingJson = "@ViewBag.InvoicingJson";
            var Contract_Dat = "@ViewBag.Contract_Date"
            var InvoicingLogJson = "@ViewBag.InvoicingLogJson"
            var AccountantLogJsons = "@ViewBag.AccountantLogJson"
            var ProjectLogJson = "@ViewBag.ProjectLogJson"
            var SalesLogJson = "@ViewBag.SalesLogJson"
            var WarehouseLogJson = "@ViewBag.WarehouseLogJson"
            var ProductionerLogJson = "@ViewBag.ProductionerLogJson"
            var UserJson = sessionStorage.getItem("User");
            function Href() {
                var reg = new RegExp("&quot;", "g"); //创建正则RegExp对象 ll
                var AccountantJsons = AccountantJson.replace(reg, '"');
                location.href = "AddAccountantLog?AccountantJson=" + AccountantJsons + "&Service=" + $("#txt_services").val();
                //alert("5555")
            }

            jQuery(document).ready(function ($) {
                $("#menu").mmenu({
                    "navbar": {
                        "title": "菜单"
                    },
                    "extensions": [
                        "pagedim-black"
                    ]
                });
            });
            $("#menu").removeClass("nav-hidden");
        </script>
        <script type="text/javascript">
            $(function () {
                var $timeline_block = $('.cd-timeline-block');
                //hide timeline blocks which are outside the viewport
                $timeline_block.each(function () {
                    if ($(this).offset().top > $(window).scrollTop() + $(window).height() * 0.75) {
                        $(this).find('.cd-timeline-img, .cd-timeline-content').addClass('is-hidden');
                    }
                });
                //on scolling, show/animate timeline blocks when enter the viewport
                $(window).on('scroll', function () {
                    $timeline_block.each(function () {
                        if ($(this).offset().top <= $(window).scrollTop() + $(window).height() * 0.75 && $(this).find('.cd-timeline-img').hasClass('is-hidden')) {
                            $(this).find('.cd-timeline-img, .cd-timeline-content').removeClass('is-hidden').addClass('bounce-in');
                        }
                    });
                });
            });
        </script>
</body>

</html>